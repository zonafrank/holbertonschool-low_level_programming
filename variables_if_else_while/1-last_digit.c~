#include <stdlib.h>
#include <time.h>
/* more headers goes there */
#include <stdio.h>

/**
 *main - entry point
 *Return: Always 0 (success)
*/
int main(void)
{
	int n;
	int lastDigit;
	char msgPart1[] = "last digit is ";
	int i;
	/*char message[];*/

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	printf("%d\n", n);
	lastDigit = abs(n) % 10;
	printf("***last digit is %d\n", lastDigit);


	for (i = 0; msgPart1[i] != '\0'; i++)
	{
		putchar(msgPart1[i]);
	}
	putchar(lastDigit);
	putchar(' ');

	if (n == 0)
	{
	}
	else if (n > 0 && n < 6)
	{
	}
	else if (n == 0)
	{
	}
	return (0);
}
